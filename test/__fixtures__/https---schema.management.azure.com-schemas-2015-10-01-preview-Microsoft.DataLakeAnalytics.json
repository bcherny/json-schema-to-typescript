    "CreateDataLakeAnalyticsAccountProperties": {
      "type": "object",
      "properties": {
        "computePolicies": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CreateComputePolicyWithAccountParameters"
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],

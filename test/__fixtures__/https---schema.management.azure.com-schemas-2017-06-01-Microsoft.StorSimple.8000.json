        },
        "encryptionKey": {
          "type": "string",
          "description": "Represents the CEK of the resource."
        },
        "encryptionKeyThumbprint": {
          "type": "string",
          "description": "Represents the Cert thumbprint that was used to encrypt the CEK."
        },
        "integrityKey": {
          "type": "string",

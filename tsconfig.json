{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "target": "es6",
    "lib": [
      "es2015",
      "es2016.array.include"
    ],
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}